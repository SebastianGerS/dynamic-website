{   
    "": {
        "controller": "Default",
        "method": "start"
    },
    "start": {
        "controller": "Default",
        "method": "start"
    },
    "login": {
        "controller": "User",
        "method": "login"
    },
    "logout": {
        "controller": "User",
        "method": "logout"
    },
    "start/logedin": {
        "controller": "Blogposts",
        "method": "getAll",
        "login": true

    },
    "start/logedin/createBlogposts": {
        "controller": "Default",
        "method": "createBlogpostPage",
        "login": true
    },
    "start/logedin/editBlogpost/:id": {
        "controller": "Blogposts",
        "method": "blogpostEditPage",
        "login": true,
        "params": {
            "id": "number"
        }
    },
    "deletePostFromDB": {
        "controller": "Blogposts",
        "method": "deletePostFromDb",
        "login": true
    },
    "edit/:id": {
        "controller": "Blogposts",
        "method": "editPostInDatabase",
        "login": true,
        "params": {
            "id": "number"
        }
    },
    "postToDatabase": {
        "controller": "Blogposts",
        "method": "insertBlogPostToDb",
        "login": true
    }, 

    "start/createUser": {
        "controller":"Default",
        "method": "createUserPage"
    },

    "createUser": {
        "controller":"User",
        "method": "createUser"
    },
    "blogposts/:page": {
        "controller": "Blogposts",
        "method": "getAllWithPage",
        "params": {
            "page": "number"
        }
    },
    "blogposts": {
        "controller": "Blogposts",
        "method": "getAll"
    },
    "blogpost/:id": {
        "controller": "Blogposts",
        "method": "getBlogpost",
        "params": {
            "id": "number"
        }
    },
    "blogposts/search": {
        "controller": "Blogposts",
        "method": "searchByTagName"
    },
    "my-blogposts": {
        "controller": "Blogposts",
        "method": "getByUser",
        "login": true
    },
    "my-blogposts/:page": {
        "controller": "Blogposts",
        "method": "getByUserWithPage",
        "login": true,
        "params": {
            "page": "number"
        }

    }
    

}